{
  "name": "ng-hackernews",
  "description": "A Hackernews implementation in Angular, with interface inspired by Nytimes.",
  "version": "2.0.0",
  "author": {
    "name": "Shaun"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "config": "node ./scripts/setenv.js",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=prod && ng build",
    "watch": "npm run config -- --environment=dev && ng build --watch --configuration development",
    "test": "npm run config -- --environment=dev && ng test",
    "coverage": "npm run config -- --environment=dev && ng test --no-watch --code-coverage",
    "format": "prettier . --write",
    "check": "prettier . --check",
    "lint:css": "stylelint **/*.sass"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "firebase": "^9.23.0",
    "rxfire": "6.0.3",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "dotenv": "^16.3.2",
    "jasmine-core": "~3.8.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^3.2.4",
    "stylelint": "^16.2.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "~5.4.5",
    "yargs": "^17.3.1"
  }
}